{
    "version": 2,
    "installCommand": "npm install",
    "buildCommand": "npm run start",
    "outputDirectory": "dist",
    "builds": [
      {
        "src": "dist/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/index.js"
      }
    ]
  }